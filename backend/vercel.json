{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "headers": {
        "Access-Control-Allow-Origin": "https://food-delivery-website-gamma.vercel.app/",
        "Access-Control-Allow-Credentials": "true"
      }
  }
